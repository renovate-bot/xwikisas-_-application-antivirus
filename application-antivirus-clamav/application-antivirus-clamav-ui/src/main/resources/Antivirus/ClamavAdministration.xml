{{html clean="false"}}
&lt;form action="$configDoc.getURL('save')" class='xform half'&gt;
  &lt;dl&gt;
    &lt;dt&gt;
      &lt;label for='Antivirus.ClamavConfigurationClass_0_host'&gt;$configClass.host.translatedPrettyName&lt;/label&gt;
